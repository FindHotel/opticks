{
  "name": "opticks",
  "version": "2.0.0",
  "description": "FindHotel Toggle Flag JavaScript SDK",
  "main": "lib/simple.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean:booleanToggle": "jscodeshift --parser flow --transform lib/transform/booleanToggle.ts",
    "clean:toggle": "jscodeshift --parser flow --transform lib/transform/toggle.ts",
    "jscodeshift": "jscodeshift",
    "prepare": "npm run build",
    "test": "xo && jest src",
    "test:watch": "jest --watch src",
    "typescript": "tsc",
    "xo": "xo",
    "xo:fix": "xo --fix"
  },
  "keywords": [
    "experiment",
    "ab testing",
    "feature-flag",
    "toggle",
    "flag",
    "canary",
    "codemod",
    "jscodeshift"
  ],
  "author": "Jop de Klein",
  "license": "ISC",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "^3.0.1",
    "jscodeshift": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@optimizely/optimizely-sdk": "^3.0.1",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^23.6.0",
    "jscodeshift": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^3.0.0",
    "typescript": "^4.0.2",
    "xo": "^0.33.0"
  }
}
