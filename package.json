{
  "name": "opticks",
  "version": "2.0.0",
  "description": "FindHotel Toggle Flag JavaScript SDK",
  "main": "lib/simple.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean:booleanToggle": "jscodeshift --parser flow --transform lib/transform/booleanToggle.ts",
    "clean:toggle": "jscodeshift --parser flow --transform lib/transform/toggle.ts",
    "jscodeshift": "jscodeshift",
    "prepare": "npm run build",
    "test": "xo && jest src",
    "test:watch": "jest src --watch",
    "typescript": "tsc --noEmit",
    "xo": "xo",
    "xo:fix": "xo --fix"
  },
  "keywords": [
    "experiment",
    "ab testing",
    "feature-flag",
    "toggle",
    "flag",
    "canary",
    "codemod",
    "jscodeshift"
  ],
  "author": "Jop de Klein",
  "license": "ISC",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "^4.2.1",
    "jscodeshift": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@optimizely/optimizely-sdk": "^4.2.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.10",
    "babel-jest": "^26.3.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4",
    "jscodeshift": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^2.26.11",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-terser": "^3.0.0",
    "typescript": "^4.0.2",
    "xo": "^0.33.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@optimizely))"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  }
}
